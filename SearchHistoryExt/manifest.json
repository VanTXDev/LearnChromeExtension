{
	"manifest_version": 3,
	"name": "Search History Extensions",
	"description": "Search History Extension",
	"version": "1.1",
	"action": {
		"default_popup": "popup.html",
		"default_icon": "images/icon.png"
	},
	"permissions": ["activeTab", "history", "nativeMessaging"],
	"background": {
		"service_worker": "background.js"
	},
	"web_accessible_resources": [{
		"resources": ["images/icon.png"],
		"matches": ["<all_urls>"]
	}]
}
